    "tinycolor2": "1.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "parcel-bundler": "^1.6.1"
  },
  "staticFiles": {
      "staticPath": "static",
      "watcherGlob": "**/*.*"
  }
}
